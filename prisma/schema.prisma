generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model recibo {
  id           String   @id @default(uuid())
  name         String
  doc          String
  description  String[]
  amount       Float[]
  total_amount Float
  issuer       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
