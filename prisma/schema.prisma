generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model recibo {
  id           String   @id @default(uuid())
  name         String
  doc          String
  description  String[]
  amount       Float[]
  total_amount Float
  issuer       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}

model empresas {
  id String @id @default(uuid())


  bairro String

  cep String

  cnaePrincipalCodigo String

  cnaePrincipalDescicao String

  cnpj String

  complemento String

  dataAbertura String

  ddd String

  email String

  logradouro String

  municipio String

  nomeFantasia String

  numero String

  razaoSocial String

  status String

  telefone String

  tipoLogradouro String

  uf String

  
}
